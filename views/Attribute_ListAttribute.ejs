<div class="content-wrapper" style="padding: 1%;">
  <div class="alert">
    <% if(message.length > 0) { %>
      <div class="alert alert-success" style="background-color: white; color: green; font-weight: 800;">
        <%= message %>
      </div>
      <% }%>
    </div>
    <div class="row">
        <div class="col-12">
    <div class="card" style="font-size: 2rem; font-weight: 800;  margin-left: 2rem; margin-right: 2rem;">
        <div class="div" style="margin-left: 15px;   padding: 2%;">
        List Medicine Attribute
        <a  class="btn"  href='/Attribute_AddAttribute?id=<%= category %>'" style="margin-top: 10px; background-color: rgb(55, 26, 82); color: white;  float: right;margin-left: -15px;"><i class="fas fa-plus-circle"></i> Add Fields</a>

        </div>
    </div>


        </div>


      
    </div>

    <!--  -->
    <div class="card-body" style="margin-top:30px; background-color: rgb(255, 255, 255); margin-left: 2rem; margin-right: 2rem;">
        <table id="example1" class="table table-bordered table-striped">
            <thead>
            <tr>
              <th>#</th> 
              <th>Field Name</th>
              <th>Field Type</th>
              <th>Position Type</th>
              <th>Number</th>
              <th>Parent</th>
             
              <th>Action</th>
          
            </tr>
            </thead>
           
         
              <%
              if(ListData.length!=0){
              var i=1;
              var fieldtype=['Number','Text','Text Area','Search Radio','Searchable Checkbox','Searchable Dropdown','Color'];
              ListData.forEach(function(data1){
              var    a=ListData.find(user => { return user.att_id==data1.isparent})  
              %>
            <tr>
              <td><%=i++; %></td>
              <td><%=data1.att_name; %></td>
              <td><%= field[data1.att_type];%></td>
              <td><%=data1.att_position==0?'Global':'Local'; %></td>
              <td><%=data1.att_number==0?'Single':'Multiple'; %></td>
              <td><%=data1.isparent==0?'Noparent': a.att_name ; %></td>
              
              <td>
                  <a href="/EditAttribute?id=<%=data1.att_id; %>"><i class="fas fa-edit" style=" color:rgb(37, 112, 250); font-size: 1.5rem;"></i></a>&nbsp;
                  <a href="/attributeDelete?id=<%=data1.att_id; %>"><i class="fas fa-trash-alt" style=" color:rgb(240, 46, 21) ; font-size: 1.5rem;"></i></a>&nbsp;
                  <% if(data1.att_position==0){ %>
                  <a href="/AttributesData?id=<%=data1.att_id; %>"> <i class="fas fa-plus" style=" color:rgb(5, 5, 5); font-size: 1.5rem;"></i></a>&nbsp;                        
           <% } %>
                </td>
            </tr>
            <% }) }%>
            </tbody>
            <tfoot>
           
            </tfoot>
          </table>
          
      </div> 

